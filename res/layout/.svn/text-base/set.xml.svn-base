<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="bottom|right"
    android:orientation="vertical" >
    
    <LinearLayout
        android:id="@+id/LinearLayout1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.13"
        android:orientation="vertical"
        android:showDividers="middle|end"
        android:divider="@drawable/abs__list_divider_holo_dark" >

       

        <TextView
            android:id="@+id/set_change"
            android:layout_height="0dp"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:textColor="@color/word"
            android:layout_marginLeft="10dp"
            android:text="@string/set2"
            android:textSize="6pt" />

        <TextView
            android:id="@+id/TextView3"
            android:layout_height="0dp"
            android:layout_marginLeft="10dp"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:textColor="@color/word"
            android:layout_weight="1"
            android:text="@string/set3"
            android:textSize="6pt" />

        <TextView
            android:id="@+id/TextView4"
            android:textColor="@color/word"
            android:layout_height="0dp"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:text="@string/set4"
            android:textSize="6pt" />

        <RelativeLayout
            android:id="@+id/TextView5"
            android:layout_height="0dp"
            
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_weight="1" >

            <TextView
                android:layout_centerVertical="true"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/set5"
                android:textColor="@color/word"
                android:layout_marginLeft="10dp"
                android:textSize="6pt" />

            <ToggleButton
                android:id="@+id/toggleButton1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="63dp" />

        </RelativeLayout>

        <TextView
            android:id="@+id/set_exit"
            android:layout_height="0dp"
            android:layout_marginLeft="10dp"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
			
			android:textColor="@color/word"
			android:layout_weight="1"
            android:text="@string/set6"
            android:textSize="6pt" />

        <TextView
            android:id="@+id/TextView7"
            android:layout_height="0dp"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:text="@string/set7"
            android:layout_marginLeft="10dp"
            android:textColor="@color/blue"
            android:textSize="9pt" />

        <TextView
            android:id="@+id/TextView8"
            android:layout_height="0dp"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:textColor="@color/word"
            android:text="@string/set8"
            android:textSize="6pt" />

        <TextView
            android:id="@+id/TextView9"
            android:layout_height="0dp"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:textColor="@color/word"
            android:layout_marginLeft="10dp"
            android:text="@string/set9"
            android:textSize="6pt" />
    </LinearLayout>

</LinearLayout>